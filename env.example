FEISHU_APP_SECRET=your_bot_app_secret
FEISHU_VERIFICATION_TOKEN=your_verification_token
FEISHU_ENCRYPT_KEY=your_encrypt_key

# 飞书API配置
# FEISHU_API_VERSION=v1  # 已固定为v1，无需配置
FEISHU_ENABLE_SIGNATURE_VERIFICATION=false  # 是否启用签名验证

# 飞书连接模式配置
FEISHU_MODE=eventsource  # 可选值: webhook, eventsource
FEISHU_BATCH_SIZE=10  # 批量处理大小
FEISHU_RETRY_ATTEMPTS=3  # 重试次数
FEISHU_PROCESS_INTERVAL=2000  # 处理间隔（毫秒）

# 服务器配置
PORT=3000
NODE_ENV=development

# API 认证（可选）
API_KEY=your_api_key_for_http_endpoints

# === 应用商店集成配置 ===
# 是否启用应用商店集成功能
ENABLE_STORE_INTEGRATION=false

# App Store Connect 集成
APPSTORE_INTEGRATION_ENABLED=false
# App Store Bundle IDs (逗号分隔)
APP_STORE_BUNDLE_IDS=com.example.app1,com.example.app2
# App Store API 速率限制 (请求/分钟)
APPSTORE_RATE_LIMIT=200
# App Store API 请求超时 (毫秒)
APPSTORE_TIMEOUT=30000

# Google Play 集成 (暂未实现)
